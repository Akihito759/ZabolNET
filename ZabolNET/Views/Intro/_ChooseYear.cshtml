@model ZabolNET.ViewModels.ChooseViewModel

<div>
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6">
            <h2 style="text-align:center; animation: ease-in" class="TextStyle">Wybierz rok rozpoczęcia studiów</h2>
        </div>
        <div class="col-sm-3"></div>
    </div>


    <div class="row">
        <div class="col-sm-2"> </div>
        <div class="col-sm-8" style="text-align:center;">
            <ul style="list-style-type:none; border: dashed" >
               
                @foreach (var el in Model.Course.Years)
                {
                    <li><button type="button" class="btn btn-primary btn-block">@el.StartYear</button></li>
                }
            </ul>
        </div>
        <div class="col-sm-2"> </div>
    </div>
</div>