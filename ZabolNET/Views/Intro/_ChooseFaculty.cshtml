@model List<string>
 
   <div>
        <div class="row">
            <div class="col-sm-3"></div>
            <div class="col-sm-6">
                <h2 style="text-align:center; animation: ease-in" class="TextStyle">Wybierz swój wydział</h2>
            </div>
            <div class="col-sm-3"></div>
        </div>


        <div class="row">
            <div class="col-sm-2"> </div>
            <div class="col-sm-8" style="text-align:center;">
                <ul style="list-style-type:none; border: dashed">
                    @foreach (var el in Model)
                    {
                        <li><button type="button" class="btn btn-primary btn-block">@el</button></li>
                    }
                </ul>
            </div>
            <div class="col-sm-2"> </div>
        </div>
    </div>
